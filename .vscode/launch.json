    "version": "0.2.0",
    "configurations": [


        {
            "name": "Mocha Mongodb",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            // "args": ["test/**/*.js", "--no-timeouts"],
            "args": ["test/mongodb.js", "--no-timeouts"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "env": { 
                "LOG_LEVEL": "error",
                "NODE_ENV": "testing",
                "DATABASE_URL": "mongodb://localhost:27017/rptest",
                // "DATABASE_USERNAME": "admin",
                // "DATABASE_PASSWORD": "admin",
                },
            "outputCapture": "std"
        },
        {
            "name": "Mocha Socket.IO",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            // "args": ["test/**/*.js", "--no-timeouts"],
            "args": ["test/sioserver.js", "--no-timeouts"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "env": { 
                "LOG_LEVEL": "error",
                "NODE_ENV": "testing",
                // "PREFIX": "rppp",
                // "PORT_SIO": "3011",
                "VUE_APP_SERVER_PORT": "3011",
                "DATABASE_URL": "mongodb://localhost:27017/rptest",
                "DATABASE_USERNAME": "admin",
                "DATABASE_PASSWORD": "admin",
                "SECRET_KEY": "MySuperSecretKey"
                },
            "outputCapture": "std"
        },
